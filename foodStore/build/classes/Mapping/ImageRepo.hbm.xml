<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.foodStore.model">
    <class name="ImageRepo" table="IMAGEREPO">
        <id name="id" column="ID">
            <generator class="sequence">
                <param name="sequence">IMAGE_ID</param>
            </generator>
        </id>
        <property name="imageCode" column="IMAGECODE"/>
        <property name="imageByte" column="IMAGEBYTE" type="byte[]"/>
    </class>
</hibernate-mapping>