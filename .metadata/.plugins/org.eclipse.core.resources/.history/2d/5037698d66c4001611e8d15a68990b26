function Main() {
	this.navbar = document.getElementById("navbar");
	this.content = document.getElementById("content");
	this.pageMangement = new PageMangement();
	pageMangement.registerPage(new TablePage());
	var thiz = this;
	navbar.addEventListener("click", function(event){
		var target = event.target;
		var pageName = target.getAttribute("page-name");
		thiz.pageManagement.activePage(pageName);
		if (thiz.pageManagement.getActivePage() == null) return;
		thiz.content.innerHTML = "";
		thiz.content.appendChild(thiz.pageManagement.getActivePage().getPageContainer);
	}, false);
	
}
window.addEventListener("load", function(){
	 var mainWindow = new Main();
} );
